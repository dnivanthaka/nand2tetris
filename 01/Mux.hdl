/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Nand(a=sel, b=sel, out=selout);
    Nand(a=a, b=selout, out=out1);
    Nand(a=sel, b=b, out=out2);
    Nand(a=out1, b=out2, out=out);
}